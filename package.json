{
  "name": "jalwa-video-server",
  "version": "2.1.0",
  "description": "P2P WebRTC Video Chat Server",
  "main": "server.js",
  "scripts": {
    "start:local": "NODE_ENV=local node server.js",
    "start:dev": "NODE_ENV=development node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop jalwa-server",
    "pm2:restart": "pm2 restart jalwa-server",
    "pm2:reload": "pm2 reload jalwa-server",
    "pm2:logs": "pm2 logs jalwa-server",
    "pm2:logs:error": "pm2 logs jalwa-server --err",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:flush": "pm2 flush",
    "pm2:save": "pm2 save",
    "test:health": "curl -f http://localhost:4000/health"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "firebase-admin": "^13.5.0",
    "ioredis": "^5.8.0",
    "socket.io": "^4.7.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^6.16.2",
    "socket.io-client": "^4.8.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
